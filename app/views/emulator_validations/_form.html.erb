<%= javascript_include_tag "forms" %>
<div class="row">
  <div class="span7">
    <%= simple_form_for [@project, @emulator_validation], html: { class: 'form-horizontal' } do |f| %>
      <%= render partial: "shared/form_slider", locals: { field: :design_size, parent: f.object_name.to_sym, object: @emulator_validation, min: 0, max: @project.emulator.validation_indices.size, step: 10, value: @emulator_validation.design_size, help: "Design remainder after training." } %>
      <div class="form-actions">
        <%= f.button :submit %>
      </div>
    <% end %>
  </div>
</div>